package Lab6;

public class Vertex implements Comparable<Vertex> {
	public String name;
	public int index;

	Vertex(String name, int index) {
		this.name = name;
		this.index = index;
	}
	
	@Override
	public int compareTo(Vertex o) {
		if (o.index<this.index)
		{
			return -1;
		}
		else if (o.index>this.index)
		{
			return 1;
		}
		else 
			return 0;
	}
	
}
